<h1>Search Results for "<%= query %>"</h1>

<h2>Users:</h2>
<ul>
  <% users.forEach(user => { %>
    <li>
      <a href="/user/<%= user.username %>"><%= user.username %></a>
    </li>
  <% }) %>
</ul>

<h2>Posts:</h2>
<ul>
  <% posts.forEach(post => { %>
    <li><%= post.caption %></li>
  <% }) %>
</ul>
